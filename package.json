{
	"name": "@coco-sheng/websocket",
	"version": "1.0.3-beta.1",
	"description": "简单易用的websocket，基于ws，遵循JSON-RPC 2.0协议",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"example": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./example/example.ts",
		"instance-server": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./multiple-instances/server.ts",
		"instance-client": "./node_modules/.bin/ts-node-dev --respawn --transpile-only ./multiple-instances/client.ts",
		"title": "./node_modules/.bin/doctoc README.md multiple-instances/README.md",
		"eslint": "eslint ./ --fix",
		"commit": "./node_modules/cz-customizable/standalone.js",
		"upgrade": "./node_modules/.bin/npm-check -u",
		"create-config": "ts-jest config:init",
		"husky-init": "husky init"
	},
	"directories": {
		"test": "test"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HandsomeTea/websocket-service.git"
	},
	"keywords": [
		"websocket"
	],
	"author": "HandsomeTea.coco@gmail.com",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/HandsomeTea/websocket-service/issues"
	},
	"homepage": "https://github.com/HandsomeTea/websocket-service#readme",
	"dependencies": {
		"log4js": "^6.9.1",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^20.12.12",
		"@types/ws": "^8.5.13",
		"cz-customizable": "^7.2.1",
		"doctoc": "^2.2.1",
		"eslint": "^9.15.0",
		"express": "^4.21.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"mongoose": "^8.8.2",
		"npm-check": "^6.0.1",
		"ts-jest": "^29.2.5",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0"
	}
